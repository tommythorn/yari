<!-- THIS IS THE PROPERTIES FILE FOR SHARP ZAURUS 5500D ONLY!
	 YOU MAY NEED TO ADJUST THAT TO YOUR PLATFORM -->
<configuration>
<properties>
<!-- INTERNAL PROPERTIES -->
<!-- Visible from Configuration.getProperty() -->
  <!-- System tunable parameters -->

  <!-- Default values for SMS internal implementation -->
  <property Key="com.sun.midp.io.j2me.sms.Impl" 
		Value="com.sun.midp.io.j2me.sms.DatagramImpl" 
		Scope="internal"
		Comment="Select the low level transport handler."/>
  <property Key="com.sun.midp.io.j2me.sms.DatagramHost" 
		Value="smsdatagramhost"
		Comment="The datagram host name."/>
  <property Key="com.sun.midp.io.j2me.sms.DatagramPortIn" 
		Value="54321" 
		Scope="internal"
		Comment="The datagram port number for inbound messages from the SMS emulator."/>
  <property Key="com.sun.midp.io.j2me.sms.DatagramPortOut" 
		Value="12345" 
		Scope="internal"
		Comment="The datagram port number of a SMS emulator."/>

  <!-- Alternate SMS port for CBS emulation -->
  <property Key="com.sun.midp.io.j2me.sms.CBSPort" 
		Value="24680" 
		Scope="internal"
		Comment="Create a CBS message connection protocol port number"/>

  <!-- Sender's phone number -->
  <property Key="com.sun.midp.io.j2me.sms.PhoneNumber" 
		Value="+5551212" 
		Scope="internal"
		Comment="The phone number for outbound messages from the SMS emulator."/>

  <!-- Default SMS Service Center address -->
  <property Key="wireless.messaging.sms.smsc" 
		Value="+17815511212" 
		Scope="internal"
		Comment="Default SMS Service Center address."/>
  <property Key="VmDebuggerPort"
		Value="2808"
		Scope="internal"
		Comment="Default SMS Service Center address port number."/>
</properties>
</configuration>
