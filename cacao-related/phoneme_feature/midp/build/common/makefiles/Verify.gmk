define verify_jsr_226_directory
    if [ "$(USE_JSR_226)" = "true" ]; then \
        $(verify_directory); \
       if [ "$(USE_PISCES)" = "false" ]; then \
           echo "ERROR: USE_PISCES should be "true""; \
           exit -1; \
       fi; \
       if [ "$(USE_JSR_172)" = "false" ]; then \
           echo "ERROR: USE_JSR_172 should be "true""; \
           exit -1; \
       fi; \
    fi
endef

