define verify_directory
    if [ -z "$($@)" ]; then \
        echo "ERROR: $@ must be set"; \
        exit -1; \
    else \
        if [ ! -d "$($@)" ]; then \
            echo "ERROR: $@ ($($@)) does not point to a valid directory"; \
            exit -1; \
        fi \
    fi
endef

define verify_satsa_directory
    if [ "$(USE_JSR_177)" = "true" ]; then \
        $(verify_directory) \
    fi
endef

define verify_jsr_211_directory
    if [ "$(USE_JSR_211)" = "true" ]; then \
